{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PgSQL": "${DATABASE_URL}"
  },
  "jwtKey": "${JWT_SECRET_KEY}",
  "EmailService": {
    "smtpServer": "${SMTP_SERVER}",
    "smtpPort": "${SMTP_PORT}",
    "smtpUser": "${SMTP_USER}",
    "smtpPassword": "${SMTP_PASSWORD}",
    "EnableSsl": true
  },
  "Genearls": {
    "UrlForgot": "${FRONTEND_URL}/auth/recover-password/token"
  },
  "PythonApiSettings": {
    "BaseUrl": "${PYTHON_API_URL}",
    "Timeout": 1520
  },
  "Encryption": {
    "Key": "${ENCRYPTION_KEY}",
    "IV": "${ENCRYPTION_IV}"
  }
}